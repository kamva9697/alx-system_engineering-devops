        <div class="content__title -no-border -raw">
            RAW Paste Data            <span class="copy__raw js-copy-raw js-tooltip" title="Copy raw to clipboard"></span>
            <span class="copy__result js-copy-success">Copied</span>
        </div>

        <textarea class="textarea -raw js-paste-raw">#!/bin/bash

push_flag=0
comment=&#039;&#039;

while getopts &#039;ps:a:c:&#039; OPTS; do
  case &quot;$OPTS&quot; in 
    p)
      push_flag=1
      ;;
    s)
      filename=&quot;${OPTARG}&quot;
      ;;
    a)
      answer=&quot;${OPTARG}&quot;
      ;;
    c)
      comment=&quot;${OPTARG}&quot;
      ;;
    *)
      echo &quot;Invalid option&quot;
      ;;
  esac
done

# echo the shebang to the script
echo &#039;#!/bin/bash&#039; &gt; &quot;${filename}&quot; 2&gt;&amp;1 

# echo the answer into the script

printf &quot;${answer}\n&quot; &gt;&gt; &quot;${filename}&quot; 2&gt;&amp;1 

# make the script executable
chmod +x &quot;${filename}&quot;

cat &quot;${filename}&quot;

if [ $push_flag -gt 0 ]; then
  # git add
  git add .

  if [ &quot;$comment&quot; != &quot;&quot;]then

    # git comment
    git commit -m &quot;$comment&quot;
  else
    # git comment default
    git commit -m &quot;Task ${filename:0:1}&quot;
  fi

  #git push
  git push
fi
</textarea>
    
        
